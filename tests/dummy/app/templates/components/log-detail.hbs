<h2> Recent Activity </h2>

<table class="logs">
  <tr>
    <th> Action </th>
    <th> Original Node </th>
    <th> User </th>
    <th> Current Node </th>
    <th> Date </th>
    <th> Additional Params Included </th>
  </tr>
  {{#each node-logs as |node-log|}}
      <tr>
        <td> {{node-log.action}} </td>
        <td> {{node-log.originalNode.id}} </td>
        <td> {{node-log.user.givenName}} </td>
        <td> {{node-log.node.id}} </td>
        <td> {{moment-format node-log.date}} </td>
        <td> {{#each-in node-log.params as |key value|}}
                {{key}},
             {{/each-in}}
        </td>
      </tr>
  {{/each}}
</table>
